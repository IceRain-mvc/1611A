<progress percent="{{num}}" show-info="true" stroke-width="20" font-size="50"></progress>
<view class='box_btn'>
<button class='btn' bindtap='sub'>-</button>
<button class='btn' bindtap='add'>+</button>
</view>

<view>
  <checkbox-group bindchange="changeBox">
    <label class='checkBox' wx:for='{{items}}'>
      <checkbox value="{{item.name}}" checked='{{item.checked}}'>{{item.name}}</checkbox>
    </label>
  </checkbox-group>
</view>

<view wx:for="{{arr}}">
  {{item}}
</view>